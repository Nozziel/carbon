{
  "version": "independent",
  "npmClient": "yarn",
  "useNx": true,
  "ignoreChanges": [
    "actions/**",
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/docs/**",
    "**/e2e/**",
    "**/examples/**",
    "**/*.md"
  ],
  "command": {
    "version": {
      "message": "chore(release): %s"
    }
  },
  "$schema": "node_modules/lerna/schemas/lerna-schema.json"
}
